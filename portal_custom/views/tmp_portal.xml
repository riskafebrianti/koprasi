<odoo>
    <template id="portal_my_home_custom" name="Show new" customize_show="True" inherit_id="portal.portal_my_home" priority="20">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">asas</t>
                <t t-set="url">/my/orderss</t>
                <!-- <t t-set="url" t-value="'/my/quotes'"/> -->
                
            </t>
          
        </xpath>
    </template>
</odoo>
